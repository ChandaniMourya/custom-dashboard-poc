{
  "definitions": [
    {
      "definitionId": "financial_overview",
      "queryId": ["financial_overview_query"],
      "typeId": "metric",
      "config": {
        "widgets": [
          {
            "widgetId": "totalDeposits",
            "label": "Total Deposits",
            "config": {
              "valueDefinition": "query_result.total_deposits",
              "trend": {
                "trendvalueDefinition": "query_result.total_deposits_trend",
                "positiveColor": "#00ff00",
                "negativeColor": "#ff0000",
                "neutralColor": "#FFFF00"
              }
            }
          },
          {
            "widgetId": "totalWithdrawals",
            "label": "Total Withdrawals",
            "config": {
              "valueDefinition": "query_result.total_withdrawals",
              "unit": "USD",
              "decimalPlaces": 2,
              "color": "#C70039",
              "trend": {
                "trendvalueDefinition": "query_result.total_withdrawals_trend",
                "positiveColor": "#00ff00",
                "negativeColor": "#ff0000",
                "neutralColor": "#FFFF00"
              }
            }
          },
          {
            "widgetId": "totalLeads",
            "label": "Total Leads",
            "config": {
              "valueDefinition": "query_result.total_leads",
              "unit": "leads",
              "decimalPlaces": 0,
              "color": "#1e90ff",
              "trend": {
                "trendvalueDefinition": "query_result.total_leads_trend",
                "positiveColor": "#00ff00",
                "negativeColor": "#ff0000",
                "neutralColor": "#FFFF00"
              }
            }
          },
          {
            "widgetId": "swapPaid",
            "label": "Swap Paid",
            "config": {
              "valueDefinition": "query_result.swap_paid",
              "unit": "USD",
              "decimalPlaces": 2,
              "color": "#ff7f50"
            }
          },
          {
            "widgetId": "positions",
            "label": "Positions",
            "config": {
              "valueDefinition": "query_result.positions",
              "type": "indicator",
              "lowerBound": 10,
              "upperBound": 100,
              "colorBelow": "#a9f75f",
              "colorAbove": "#C70039"
            }
          },
          {
            "widgetId": "newClients",
            "label": "New Clients",
            "config": {
              "valueDefinition": "query_result.new_clients",
              "unit": "clients",
              "decimalPlaces": 0,
              "color": "#ffa500"
            }
          }
